snippet ( "add closing bracket" i
( ${1} )$0
endsnippet

snippet { "add closing curly bracket" i
{
  ${1}
}$0
endsnippet

snippet [ "add closing square bracket" i
[ ${1} ]$0
endsnippet

snippet " "add closing quote" i
"$1"$0
endsnippet

snippet ' "add closing single quote" i
'$1'$0
endsnippet

snippet = "equal sign with spaces around it" i
 = $0
endsnippet
