{
  "figure": {
    "prefix": "fig",
    "body": "\\begin{figure}[H]%
\\begin{center}%
\\includegraphics[width=\\textwidth]{${1:file}}%
\\end{center}%
\\caption{${2:caption}}%
\\label{fig_${3:label}}%
\\end{figure}%"
  },
  "enumerate": {
    "prefix": "enum",
    "body": "\\begin{enumerate}%
  ${1}
\\end{enumerate}%"
  },
  "item": {
    "prefix": "itm",
    "body": "\\item ${1}"
  },
  "frame": {
    "prefix": "frame",
    "body": "\\begin{frame}%
  \\frametitle{${1:title}}%
\\end{frame}%"
  },
  "columns": {
    "prefix": "cols",
    "body": "\\begin{columns}
  \\column{${1:0.5}\\textwidth}
    ${3:col_1_content}
  \\column{${2:0.5}\\textwidth}
    ${4:col_1_content}
\\end{columns}"
  }
}
